<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.aat920.steptracker.MainActivityLauncher"
    tools:showIn="@layout/app_bar_main_activity_launcher"
    android:id="@+id/mainlayout">
<ImageView
    android:layout_width="120dp"
    android:layout_height="wrap_content"
    app:srcCompat="@drawable/rightfeet"
    android:layout_marginTop="40dp"
    android:layout_marginLeft="10dp"
    android:id="@+id/imageView1" />
<ImageView
    android:layout_width="120dp"
    android:layout_height="wrap_content"
    app:srcCompat="@drawable/leftfeet"
    android:layout_marginTop="40dp"
    android:layout_alignParentRight="true"
    android:layout_marginRight="10dp"
    android:id="@+id/imageView2" />
    <!--<ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/heel_foot"
        android:layout_marginLeft="80dp"
        android:layout_centerVertical="true"
       />-->
    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginTop="180dp"
        android:layout_marginLeft="110dp"
        android:max="200"
        android:progress="160"
        android:progressDrawable="@drawable/circcular" />

</RelativeLayout>
